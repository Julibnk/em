{
  "name": "em-backend",
  "version": "1.0.0",
  "description": "EM - Backend",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:test",
    "start:development": "env-cmd -f .env.development nodemon start.ts",
    "start:test": "env-cmd -f .env.test nodemon start.ts",
    "start:production": "env-cmd -f .env nodemon start.ts",
    "test": "jest",
    "test:local": "env-cmd -f .env.test jest",
    "build": "tsc --build",
    "build:production": "npm ci && npm run build",
    "prisma:generate:development": "env-cmd -f .env.development prisma generate",
    "prisma:generate:test": "env-cmd -f .env.test prisma generate",
    "prisma:generate:production": "env-cmd -f .env prisma generate",
    "prisma:migrate:development": "env-cmd -f .env.development prisma migrate dev",
    "prisma:migrate:test": "env-cmd -f .env.test -- prisma migrate deploy",
    "docker:up:development": "docker-compose -f ./docker-compose-development.yml up -d",
    "docker:up:test": "docker-compose -f ./docker-compose-test.yml up -d",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julibnk/em-backend.git"
  },
  "author": "Julián Fernández Fernández",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Julibnk/em-backend/issues"
  },
  "homepage": "https://github.com/Julibnk/em-backend#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/compression": "^1.7.2",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.5.0",
    "faker": "^5.5.3",
    "jest": "^29.2.0",
    "prisma": "^4.4.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@types/uuid": "^8.3.4",
    "ajv": "^8.11.0",
    "app-root-path": "^3.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "express-validator": "^6.14.2",
    "glob": "^8.0.3",
    "helmet": "^5.1.0",
    "http-status": "^1.5.2",
    "i": "^0.3.7",
    "inversify": "^6.0.1",
    "log4js": "^6.6.0",
    "npm": "^8.19.2",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  }
}
